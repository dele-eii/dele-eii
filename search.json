---
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | strip_html | escape }}",
      "url"      : "{{site.url}}{{ site.baseurl }}{{ post.url }}",
      "image"    : "{{site.url}}{{ site.baseurl }}{{ post.image }}",
      "date"     : "{{ post.date | date: "%d/%m/%Y" }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
